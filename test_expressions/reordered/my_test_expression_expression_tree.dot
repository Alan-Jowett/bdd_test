digraph ExpressionTree {
    rankdir=TB;
    node [fontname="Arial"];
    edge [fontname="Arial"];

    node0 [label="AND", shape=box, style=filled, fillcolor=lightgreen];
    node1 [label="NOT", shape=box, style=filled, fillcolor=yellow];
    node2 [label="OR", shape=box, style=filled, fillcolor=lightcoral];
    node3 [label="a", shape=ellipse, style=filled, fillcolor=lightblue];
    node2 -> node3 [label="L"];
    node4 [label="b", shape=ellipse, style=filled, fillcolor=lightblue];
    node2 -> node4 [label="R"];
    node1 -> node2;
    node0 -> node1 [label="L"];
    node5 [label="c", shape=ellipse, style=filled, fillcolor=lightblue];
    node0 -> node5 [label="R"];
}
