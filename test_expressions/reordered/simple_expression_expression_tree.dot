digraph ExpressionTree {
    rankdir=TB;
    node [fontname="Arial"];
    edge [fontname="Arial"];

    node0 [label = "a", shape=ellipse, style = filled, fillcolor = lightblue];
    node1 [label = "b", shape=ellipse, style = filled, fillcolor = lightblue];
    node2 [label = "AND", shape=box, style = filled, fillcolor = lightgreen];
    node3 [label = "c", shape=ellipse, style = filled, fillcolor = lightblue];
    node4 [label = "d", shape=ellipse, style = filled, fillcolor = lightblue];
    node5 [label = "AND", shape=box, style = filled, fillcolor = lightgreen];
    node6 [label = "OR", shape=box, style = filled, fillcolor = lightcoral];

    node2 -> node0 [label = "L"];
    node2 -> node1 [label = "R"];
    node5 -> node3 [label = "L"];
    node5 -> node4 [label = "R"];
    node6 -> node2 [label = "L"];
    node6 -> node5 [label = "R"];
}
