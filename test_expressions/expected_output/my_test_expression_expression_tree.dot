digraph ExpressionTree {
    rankdir=TB;
    node [fontname="Arial"];
    edge [fontname="Arial"];

    node0 [label = "a", shape=ellipse, style = filled, fillcolor = lightblue];
    node1 [label = "b", shape=ellipse, style = filled, fillcolor = lightblue];
    node2 [label = "OR", shape=box, style = filled, fillcolor = lightcoral];
    node3 [label = "NOT", shape=box, style = filled, fillcolor = yellow];
    node4 [label = "c", shape=ellipse, style = filled, fillcolor = lightblue];
    node5 [label = "AND", shape=box, style = filled, fillcolor = lightgreen];

    node5 -> node3 [label = "L"];
    node3 -> node2;
    node2 -> node0 [label = "L"];
    node2 -> node1 [label = "R"];
    node5 -> node4 [label = "R"];
}
